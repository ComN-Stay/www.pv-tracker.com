{{ form_start(form) }}
<div class="row">
	<div class="col-md-4 col-xs-12">
		<div class="jumbotron">
			<h3 class="text-center">
				<a id="left" href="#">
					<i class="fa fa-chevron-left"></i>
				</a>
				<span>&nbsp;</span>
				<span id="month"></span>
				<span>&nbsp;</span>
				<span id="year"></span>
				<span>&nbsp;</span>
				<a id="right" href="#">
					<i class="fa fa-chevron-right"></i>
				</a>
				<span>&nbsp;</span>
			</h3>
		</div>
		<table class="table calendar_table"></table>
	</div>
	<div class="col-md-8 col-xs-12 mt-4">
		{{ form_row(form.production) }}
		{{ form_row(form.import) }}
		{{ form_row(form.export) }}
		<div class="hidden">
			{{ form_row(form.date) }}
		</div>
	</div>
</div>
<button class="btn btn-success btn-lg">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
