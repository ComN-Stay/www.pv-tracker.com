{% extends 'base.html.twig' %}

{% block title %}Systems{% endblock %}

{% block body %}
    <h1>Systems</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ system.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ system.name }}</td>
            </tr>
            <tr>
                <th>System_id</th>
                <td>{{ system.systemId }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ system.token }}</td>
            </tr>
            <tr>
                <th>Refresh_token</th>
                <td>{{ system.refreshToken }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_systems_index') }}">back to list</a>

    <a href="{{ path('app_systems_edit', {'id': system.id}) }}">edit</a>

    {{ include('systems/_delete_form.html.twig') }}
{% endblock %}
